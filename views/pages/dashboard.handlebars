{{> header}}

<div>
    {{#each posts}}
        <div>
            <h2>{{title}}</h2>
            <p>{{content}}</p>
            <p>Created on {{created_at}}</p>
            <button>Edit</button>
            <button>Delete</button>
        </div>
    {{/each}}
</div>
<button>Create new post</button>
